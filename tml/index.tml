<html xmlns="http://www.w3.org/1999/xhtml">
<head>
	<title>Главная страница</title>
    <script>
        function refresh() {
            location.reload();
        }
    </script>
</head>
<body onload='setInterval(function(){refresh()}, ${refreshPeriod} );'>
    <p id="auth_done">${auth_done}</p>
    <p id="status">${status}</p>
    <p><a href="/login">Авторизация</a></p>
    <p><a href="/register">Регистрация</a></p>

    <img src="jetty-logo-80x22.png"/>

</body>
</html>
